#include<stdio.h>
#include<stdlib.h>
int array_sum(int arr[], int n)
{
    int i,sum=0;
    for(i=0;i<n;i++)
    {
       sum=sum+arr[i] ;
    }
    return sum;
}
int main(){
    int arr[1000];
    int n,i,sum;
    //printf("Enter the number of elements: ");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d\t",&arr[i]);
    }
    sum=array_sum(arr,n);
    printf("%d",sum);
   
    return 0;
}
    

